<Window x:Class="PhotoBeanApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PhotoBeanApp"
        mc:Ignorable="d"
        WindowStyle="None"
        WindowState="Maximized">
    <Grid>
        <Label Content="30" FontSize="40"  Name="totalTime" VerticalAlignment="Top" HorizontalAlignment="Left" Panel.ZIndex="2" Margin="20 20 20 20"/>
        <Grid Panel.ZIndex="1">
            <ContentControl x:Name="contentControl"></ContentControl>
        </Grid>
    </Grid>
</Window>
